<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FacebookSuite">


    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnv" value="saucelabs"/>
    <parameter name="os" value="Mac"/>
    <parameter name="url" value="http://www.facebook.com/"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="50"/>
    <parameter name="testName" value="Facebook HomePage Test"/>

    <test name = "Facebook Home Page">
        <classes>
            <class name="TestLandingPage.TestLogin"/>
            <!--<class name="page.TestHomePage"/>-->
            <!--<class name="page.TestLoginPage"/>-->
        </classes>
    </test>

</suite>
